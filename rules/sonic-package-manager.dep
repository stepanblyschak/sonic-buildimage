
SPATH       := $($(SONIC_PACKAGE_MANAGER)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/sonic-package-manager.mk rules/sonic-package-manager.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(SONIC_PACKAGE_MANAGER)_CACHE_MODE  := GIT_CONTENT_SHA
$(SONIC_PACKAGE_MANAGER)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SONIC_PACKAGE_MANAGER)_DEP_FILES   := $(DEP_FILES)
$(SONIC_PACKAGE_MANAGER)_SMDEP_FILES := $(SMDEP_FILES)
$(SONIC_PACKAGE_MANAGER)_SMDEP_PATHS := $(SPATH)

