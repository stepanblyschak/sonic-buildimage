SPATH       := $($(SONIC_PACKAGE_MANAGER_DATA)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/sonic-package-manager-data.mk rules/sonic-package-manager-data.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
