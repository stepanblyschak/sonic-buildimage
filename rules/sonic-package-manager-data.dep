$(SONIC_PACKAGE_MANAGER_DATA)_CACHE_MODE  := GIT_CONTENT_SHA
$(SONIC_PACKAGE_MANAGER_DATA)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SONIC_PACKAGE_MANAGER_DATA)_DEP_FILES   := $(DEP_FILES)

